{
  "plugins": [
    "@babel/plugin-proposal-object-rest-spread",
    ["@babel/plugin-transform-react-jsx", {
      pragma: "Component.createElement",
      pragmaFrag: "Component.createFragment"
    }],
    "@babel/plugin-proposal-class-properties"
  ],
  "presets": [
    ["@babel/preset-env", {
      "modules": false
    }]
  ],
  exclude: 'node_modules/**'
}
